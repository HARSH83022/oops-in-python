# parameters passing direct execute
class faculty:
  def __init__(self,name,id,salary):
   self.name = name
   self.id = id 
   self.salary= salary
  def display(self):
    print("faculty id: ", self.name)
    print("faculty name: ", self.id)
    print("faculty salary:", self.salary)
a= faculty("harsh",2,1000000000)

b=faculty("aaku",3,1200000)
c=faculty("bhoom",4,676888)
a.display()
b.display()
c.display()
